{
  "name": "@purrfect-firs/data",
  "version": "1.0.0",
  "description": "ðŸ–– D.A.T.A. - Database Automation, Testing, and Alignment for PostgreSQL/Supabase",
  "type": "module",
  "main": "src/index.js",
  "bin": {
    "data": "./bin/data.js"
  },
  "scripts": {
    "postinstall": "./scripts/setup/post-install.sh",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "migrate:generate": "data db migrate generate",
    "migrate:test": "data db migrate test",
    "migrate:promote": "data db migrate promote",
    "migrate:status": "data db migrate status",
    "migrate:rollback": "data db migrate rollback",
    "migrate:clean": "data db migrate clean",
    "migrate:history": "data db migrate history",
    "migrate:verify": "data db migrate verify",
    "migrate:squash": "data db migrate squash",
    "migrate:dev": "npm run migrate:generate && npm run migrate:test",
    "migrate:prod": "npm run migrate:test && npm run migrate:promote",
    "migrate:ci": "npm run migrate:verify && npm run migrate:test"
  },
  "keywords": [
    "supabase",
    "postgresql",
    "cli",
    "database",
    "migration",
    "admin"
  ],
  "author": "Purrfect Firs Development Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.45.0",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.11.0",
    "chalk": "^4.1.2",
    "chokidar": "^4.0.3",
    "commander": "^12.0.0",
    "dotenv": "^16.4.5",
    "figlet": "^1.7.0",
    "ink": "^5.0.1",
    "ink-select-input": "^6.0.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "inquirer": "^10.0.0",
    "oh-my-logo": "^0.3.0",
    "pg": "^8.12.0",
    "pino": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "react": "^18.3.1",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitest/coverage-v8": "^2.0.0",
    "eslint": "^9.34.0",
    "eslint-plugin-promise": "^7.2.1",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.0.0"
  },
  "workspaces": [
    "packages/*"
  ]
}
